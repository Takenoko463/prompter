<div class="prompt">
  <div class="prompt-header">
    <div class="prompt-title">
      <%=prompt.title%>
    </div>
  </div>
  <div class="prompt-main">
    <div class="prompt-content">
      <span class="title-column">prompt内容</span>
      <p class="toggle"><%=prompt.content%></p>
    </div>
  </div>
  <div class="prompt-footer">
    <a class="prompt-nick-name"> <%=prompt.nick_name%> </a>
    <%=link_to prompt.ai.name,prompt.ai.url,class:"ai-link",target: :_blank, rel: "noopener noreferrer"%>
    <button class="content-copy-button">COPY!</button>
  </div>
</div>