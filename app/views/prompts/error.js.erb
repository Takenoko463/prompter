const $offcanvasPromptForm=$('#offcanvasPromptForm');
const $formBody=$offcanvasPromptForm.find('.offcanvas-body');
var $errorsBody=$("<ul>", {
                class: "error-alert"
                }
                ).prependTo($formBody);
<% @errors.each do |error| %>
$errorsBody.append(
                $("<li>",{
  class:"error-message alert alert-danger",
  role:"alert"
  })
  .append("<%= j(error).html_safe %>")
  );
<% end %>