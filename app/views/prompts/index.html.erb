<main class="mt-3 py-2">
  <div class="container-lg">
    <div class="col-md-auto">
      <h1 class="text-center  mt-2">プロンプト一覧</h1>
      <h2  class="text-center mb-2"><%=@category.name%></h2>
      <%if @prompts.present?%>
        <%=render partial:"prompts/prompt", collection:@prompts%>
      <%else%>
        <p class="text-center">対象プロントは存在しません</p>
      <%end%>
    </div>
  </div>
</main>