<%= search_form_for q, url: url do |f| %>
  <div class="form-group">
    <%= f.label :content_cont, "プロンプト内容検索" %>
    <%= f.search_field :content_cont, class: 'form-control', placeholder: '検索ワード' %>
  </div>
  <div class="form-group">
    <%= f.label :subtree_category, "カテゴリ検索" %>
    <%= f.select :subtree_category, options_from_collection_for_select(Category.all, :id, :name , 0),class:"form-control form-select"%>
  </div>
  <%= f.submit '検索', class: 'btn btn-primary' %>
<% end %>
